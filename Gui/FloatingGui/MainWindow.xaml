<Window x:Class="Instrument.Gui.FloatingGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:gui="http://schemas.instrument.net/wpf/xaml/gui"
        Title="MainWindow" Height="500" Width="800">
    
    <gui:DockManager>
        <gui:LayoutPanel>
            <gui:LayoutPanel gui:LayoutPanel.Dock="Bottom"/>
            <gui:LayoutPanel gui:LayoutPanel.Dock="Top"/>
        </gui:LayoutPanel>
    </gui:DockManager>
    
</Window>
